{
  "manifest_version": 3,
   "name": "Add My De:light",
  "version": "1.0.0",
  "key" : "",
  "description": "Automatically adds extracurricular application schedules to Google Calendar, iOS Calendar.",
  "icons": {
    "48": "images/default_icon.png",
    "128": "images/default_icon.png"
  },

  "permissions": [
    "storage",
    "tabs",
    "identity"
  ],

   "host_permissions": [
    "https://delight.duksung.ac.kr/*",
    "https://www.googleapis.com/*"
  ],

  "oauth2": {
    "redirect_uri": "https://idphbbadocicknjccgmggjgnhikjlaok.chromiumapp.org/google",
    "client_id": "924893343928-c26c1vog850sljsdg4aqtiv0gi8fbigm.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.events"
    ]
  },

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_popup": "popup/popup.html"
  },

  "content_scripts": [
  {
    "matches": ["https://delight.duksung.ac.kr/*"],
    "js": ["scripts/content.js"],
    "run_at": "document_end"
  }
  ]

}
