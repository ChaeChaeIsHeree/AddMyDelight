{
  "manifest_version": 3,
  "name": "Minimal Manifest",
  "version": "1.0.0",
  "description": "Automatically adds extracurricular application schedules to Google Calendar.",
  "icons": {
    "48": "images/default_icon.png",
    "128": "images/default_icon.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "identity"
  ],
  "host_permissions": [
    "https://delight.duksung.ac.kr/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
